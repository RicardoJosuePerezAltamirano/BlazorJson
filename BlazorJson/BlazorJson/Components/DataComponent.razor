@rendermode InteractiveServer
@if(DataModel!=null)
{
    <table>
        <thead>
            <tr>
                <td>nombre</td><td>imagen</td><td>breed</td><td>type</td><td>localization</td><td>size</td><td>gender</td><td>health</td><td>meet</td><td>social</td><td>url</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in DataModel)
            {
                <tr>
                    <td>@item.Name</td>
                    <td> <img src="@item.Image" width="200" height="200" /> </td>
                    <td>@item.Breed</td>
                    <td>@item.Type</td>
                    <td>@item.Location</td>
                    <td>@item.Size</td>
                    <td>@item.Gender</td>
                    <td>@item.Health</td>
                    <td>@item.Meet</td>
                    <td>@item.Social</td>
                    <td> <a href="@item.Url">link</a> </td>
                </tr>
            }

        </tbody>
    </table>
}